    "css": {
        ".nav-button.ng-scope.selected": {

        },
        ".notifications__item-image": {
            "filter": "invert(100%)"
        },
        ".ivu-chart-circle": {
            "filter": "brightness(1.5) contrast(2)"
        },
        ".ivu-icon.no-tasks-assessments-icon": {
            "filter": "invert(100%)"
        },
        ".ep-border-button": {
            "border": "none"
        },
        ".subject-card .icon.tight": {
            "filter": "contrast(0.75)"
        }
    }
